//DELETE THE RECORD OF INVOICE_ITEM TABLE ITS PURCHASE QUANTITY IS HIGH

mysql> select * from invoice_item;
+--------+------+----------+
| inv_id | pid  | quantity |
+--------+------+----------+
|    102 |  222 | 6        |
|    103 |  333 | 2        |
|    105 |  555 | 2        |
+--------+------+----------+

mysql> delete from invoice_item where quantity=6;
Query OK, 1 row affected (0.00 sec)

mysql> select * from invoice_item;
+--------+------+----------+
| inv_id | pid  | quantity |
+--------+------+----------+
|    103 |  333 | 2        |
|    105 |  555 | 2        |
+--------+------+----------+

